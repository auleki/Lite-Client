{
  "name": "morpheus",
  "productName": "Morpheus",
  "version": "0.7.4-daily.5",
  "description": "Morpheus is private, sovereign, AI",
  "main": ".webpack/main",
  "scripts": {
    "_comment": "GITHUB RELEASES: Use ./release-github.sh script with existing signed artifacts (never rebuild)",
    "preinstall": "node .setup/yarn-preinstall-system-validation.mjs",
    "start": "cross-env NODE_ENV=development DEBUG=electron-packager electron-forge start",
    "package": "set DEBUG=electron-packager && electron-forge package",
    "make": "electron-forge make",
    "_comment_build": "BUILD: Creates signed, notarized, and stapled artifacts (run once when code changes)",
    "build": "yarn make",
    "_comment_release": "GITHUB RELEASES: Use custom script that uploads existing artifacts without rebuilding",
    "release:daily": "./release-github.sh daily",
    "release:patch": "./release-github.sh patch",
    "release:minor": "./release-github.sh minor",
    "release:major": "./release-github.sh major",
    "lint": "eslint --ext .ts,.tsx .",
    "pretty": "npx prettier --no-error-on-unmatched-pattern src/**/*.{js,jsx,ts,tsx,json,css} --write",
    "prepare": "husky"
  },
  "prettier": {
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "printWidth": 100
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run pretty"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "keywords": [],
  "author": {
    "name": "Morpheus",
    "email": "morpheusai@proton.me"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-dmg": "^7.2.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-zip": "^7.2.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.2.0",
    "@electron-forge/plugin-webpack": "^7.2.0",
    "@electron-forge/publisher-github": "^7.2.0",
    "@svgr/webpack": "^8.1.0",
    "@types/ajv": "^1.0.0",
    "@types/qrcode": "^1.5.5",
    "@types/react-dom": "^18.2.19",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-transition-group": "^4.4.10",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vercel/webpack-asset-relocator-loader": "1.7.3",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.0.0",
    "electron": "28.1.4",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.25.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "husky": "^9.0.5",
    "lint-staged": "^15.2.2",
    "node-loader": "^2.0.0",
    "prettier": "^3.2.4",
    "style-loader": "^3.0.0",
    "ts-loader": "^9.2.2",
    "ts-node": "^10.0.0",
    "typescript": "~4.5.4",
    "webpack-permissions-plugin": "^1.0.9"
  },
  "dependencies": {
    "@metamask/sdk": "^0.14.2",
    "@metamask/sdk-react": "^0.14.2",
    "@metamask/sdk-react-ui": "^0.14.3",
    "@types/react": "^19.1.9",
    "axios": "^1.11.0",
    "check-disk-space": "^3.4.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.1.0",
    "ethers": "^6.11.1",
    "lucide-react": "^0.536.0",
    "ollama": "^0.4.3",
    "qrcode": "^1.5.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-loader-spinner": "^6.1.6",
    "react-router-dom": "^6.21.3",
    "react-transition-group": "^4.4.5",
    "styled-components": "^6.1.8",
    "sudo-prompt": "^9.2.1",
    "winston": "^3.11.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
